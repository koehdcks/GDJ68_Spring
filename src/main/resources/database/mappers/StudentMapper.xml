<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


 	<mapper namespace="com.iu.main.student.StudentDAO">
 		<select id="getList" resultType="com.iu.main.student.StudentDTO">
 			SELECT * FROM STUDENT ORDER BY STUNUM DESC
 		</select>
 		<select id="getDetail" resultType="com.iu.main.student.StudentDTO" parameterType="com.iu.main.student.StudentDTO">
 			SELECT * FROM STUDENT WHERE STUNUM=#{stuNum}
 		</select>
 	</mapper>